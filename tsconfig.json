{
  "compileOnSave": false,
  "compilerOptions": {
      "module": "commonjs",
      "moduleResolution": "node",
      "target": "ES2016",
      "noImplicitAny": true,
      "sourceMap": false,
      "importHelpers": true,
      "inlineSourceMap": true,
      "inlineSources": true,
      "baseUrl": ".",
      "skipLibCheck": true,
      "sourceRoot": ".",
      "types": [
        "index.d.ts"
      ]
  },
  "exclude": [ "blueprints/*/files/**", "dist", "tmp"]
}
